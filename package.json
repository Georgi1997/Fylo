{
  "name": "fylo-dark-theme-landing-page-master",
  "version": "1.0.0",
  "description": "![Design preview for the Fylo landing page with dark theme and features grid challenge](./design/desktop-preview.jpg)",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server --browser='Google Chrome'",
    "start": "npm-run-all --parallel devserver watch:sass",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.css  -o css/style.prefix.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "postcss": "^8.2.4"
  }
}
